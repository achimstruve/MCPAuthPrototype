# Environment variables (may contain secrets like JWT_SECRET_KEY)
.env

# Python virtual environment (created by `uv sync`)
.venv/

# Python bytecode cache (generated at import time)
__pycache__/
*.pyc
*.pyo

# Distribution / packaging artifacts
dist/
build/
*.egg-info/

# Test and linter caches
.pytest_cache/
.ruff_cache/
.coverage
htmlcov/

# IDE settings
.idea/
.vscode/
*.swp
*.swo

# Claude Code local settings (user-specific permissions, not project config)
.claude/settings.local.json

# OS files
.DS_Store
Thumbs.db

# =============================================================================
# Terraform
# =============================================================================
# Local .terraform directories (provider plugins, modules)
**/.terraform/*

# Terraform state files (contain sensitive data like resource IDs, outputs)
*.tfstate
*.tfstate.*

# Crash log files
crash.log
crash.*.log

# Variable definitions (may contain sensitive values)
*.tfvars
*.tfvars.json

# Keep the example file
!*.tfvars.example

# Override files (local development overrides)
override.tf
override.tf.json
*_override.tf
*_override.tf.json

# CLI configuration files
.terraformrc
terraform.rc

# Lock file (can be committed, but we exclude for simplicity)
.terraform.lock.hcl
